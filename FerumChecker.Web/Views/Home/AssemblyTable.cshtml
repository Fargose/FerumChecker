<table class="table" id="assembly-table">
    <thead>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr data-id="@item.Id">
                <td style="width: 100%">
                    <div class="catalog-item">
                        <div class="item-name">
                            <i class="fas fa-desktop" aria-hidden="true"></i>
                            @item.Name
                            <span style="float: right">
                                Автор: @item.OwnerName
                                <a asp-action="Details" asp-controller="Editor" asp-route-id="@item.Id"><i class="fas fa-eye" aria-hidden="true"></i></a>
                            </span>
                        </div>
                    </div>
                </td>
                @*<td style="width: 30%">
                        <a class='edit-link' asp-controller="VideoCard" asp-action="edit" asp-route-id="@item.Id"><i class='fas fa-edit'></i></a>
                        <a class='delete-link'><i class='fas fa-trash'></i></a>
                    </td>*@
            </tr>
        }
    </tbody>
</table>